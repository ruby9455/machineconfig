{
  "tools": {
    "autoAccept": true,
    "core": [
      "EditTool",
      "GlobTool",
      "WebSearchTool",
      "ReadFileTool",
      "LSTool",
      "ReadManyFilesTool",
      "MemoryTool",
      "GrepTool",
      "ShellTool",
      "WebFetchTool",
      "WriteFileTool"
    ],
    "exclude": [],
    "sandbox": false
  },
  "advanced": {
    "bugCommand": {
      "urlTemplate": "https://github.com/google-gemini/gemini-cli/issues/new?template=bug_report.yml&title={title}&info={info}"
    }
  },
  "model": {
    "chatCompression": {
      "contextPercentageThreshold": 0.5
    }
  },
  "context": {
    "fileName": "GEMINI.md",
    "fileFiltering": {
      "respectGitIgnore": true,
      "enableRecursiveFileSearch": true
    },
    "includeDirectories": []
  },
  "ui": {
    "customThemes": {
      "MyCustomTheme": {
        "name": "MyCustomTheme",
        "type": "CMD",
        "Background": "#181818",
        "Foreground": "#F8F8F2",
        "AccentBlue": "#61AFEF",
        "AccentPurple": "#C678DD",
        "AccentGreen": "#98C379"
      }
    },
    "theme": "Default"
  },
  "mcpServers": {},
  "general": {
    "preferredEditor": "vim"
  },
  "security": {
    "auth": {
      "selectedType": "gemini-api-key"
    }
  },
  "telemetry": {
    "enabled": false,
    "target": "remote",
    "logPrompts": false
  },
  "privacy": {
    "usageStatisticsEnabled": false
  }
}